<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity ParamArticle.
    -->
    <changeSet id="20210301080431-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="famille_id"
                                 baseTableName="param_article"
                                 constraintName="fk_param_article_famille_id"
                                 referencedColumnNames="id"
                                 referencedTableName="param_famille_article"/>

        <addForeignKeyConstraint baseColumnNames="unite_id"
                                 baseTableName="param_article"
                                 constraintName="fk_param_article_unite_id"
                                 referencedColumnNames="id"
                                 referencedTableName="param_unite_mesure"/>

    </changeSet>
</databaseChangeLog>
